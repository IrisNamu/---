CREATE TABLE LOGIN(
	user_name varchar2(30) ,
	ID VARCHAR2(30),
	PASSWORD VARCHAR2(30) , --占쎈쐻占쎈윥占쎈㎍占쎈쐻占쎈윥占쎌몗占쎈쐻占쎈윥�땻�뼹�쐻占쎈윥占쎈㎍占쎈쐻占쎈윥占쎌몗占쎈쐪筌먦끉�룯�뜝�럩留띰옙�쐻占쎈윥占쎌몝占쎈쐻占쎈윪筌띲깷�쐻占쎈윥占쎈㎍占쎈쐻占쎈윥占쎌몗�뜝�럥夷ⓨ뜝�럥�꽣�뜝�럩留띰옙�쐻占쎈윥占쎌몗占쎈쐻占쎈윥�뤃占� �뜝�럥�맶�뜝�럥�쑅�뜝�럥�걖�뜝�럥�맶�뜝�럥�쑅�뜝�럥彛뉐뜝�럥�맶�뜝�럥�쐾�뜝�럥占싼껊쐻占쎈윥鸚롤껊쐻占쎈윥筌앷엥�쐻占쎈윥�댚占썲뜝�럥�맶�뜝�럥�쑅�뜝�럥�삙占쎈쐻占쎈윥占쎈㎍占쎈쐻占쎈윥占쎌몝癲ル슢�뀖獄�釉�鍮듿뜝�뜴�뒙占쎈낄嫄쇿뜝�럡�렊 占쎈쐻占쎈윥占쎈㎍占쎈쐻占쎈윥占쎌몗占쎈쐻占쎈윥占쎈폆占쎈쐻占쎈윥占쎈㎍占쎈쐻占쎈윥占쎌몗傭��끉猷딉옙�굲
	mail varchar2(60),
	CONSTRAINT PK_ID PRIMARY KEY(ID)
)

