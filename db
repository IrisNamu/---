SELECT * FROM tab


CREATE TABLE STUDENT(
	stuNumber VARCHAR2(4) NOT NULL,
	stuName VARCHAR2(10) NOT NULL,
	sex VARCHAR2(5),
	age varchar2(5),
	school varchar2(30),
	grade varchar2(35),
	className varchar2(30),
	birth varchar2(30),
	when_day varchar2(100),
	address VARCHAR2(100),
	enter_date varchar2(30),
	student_call VARCHAR2(100),
	guardian1 VARCHAR2(100),
	guardian1_call VARCHAR2(100),
	guardian2 VARCHAR2(100),
	guardian2_call VARCHAR2(100),
	stu_memo varchar2(300),
	pic VARCHAR2(100),
	CONSTRAINT PK_stuNumber PRIMARY KEY(stuNumber)
)

