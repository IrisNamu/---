FROM STUDENT s LEFT OUTER JOIN ATTENDANCE a 
USING (stunumber)
WHERE today LIKE '%2022�뜝�럥�맶�뜝�럥�쑅占쎈쐻占쎈짗占쎌굲 07�뜝�럥�맶�뜝�럥�쑋�뜝�럩留�%'

