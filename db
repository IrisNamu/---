INSERT INTO LOGIN VALUES('green', 'green1234');

SELECT * FROM LOGIN WHERE id = 'green' AND password = 'green1234';

CREATE TABLE LOGIN(
	ID VARCHAR2(30),
	PASSWORD VARCHAR2(30) NOT NULL, 윥占쎌몗占쎈쐻占쎈윥�뤃占� �뜝�럥�맶�뜝�럥�쑅�뜝�럥�걖�뜝�럥�맶�뜝�럥�쑅�뜝�럥彛뉐뜝�럥�맶�뜝�럥�쐾�뜝�럥占싼껊쐻占쎈윥鸚롤껊쐻占쎈윥筌앷엥�쐻占쎈윥�댚占썲뜝�럥�맶�뜝�럥�쑅�뜝�럥�삙占쎈쐻占쎈윥占쎈㎍占쎈쐻占쎈윥占쎌몝癲ル슢�뀖獄�釉�鍮듿뜝�뜴�뒙占쎈낄嫄쇿뜝�럡�렊 占쎈쐻占쎈윥占쎈㎍占쎈쐻占쎈윥占쎌몗占쎈쐻占쎈윥占쎈폆占쎈쐻占쎈윥占쎈㎍占쎈쐻占쎈윥占쎌몗傭��끉猷딉옙�굲
	mail varchar2(50)
	CONSTRAINT PK_ID PRIMARY KEY(ID)
)

COMMIT;

SELECT * FROM LOGIN

SELECT * FROM LOGIN;

ALTER TABLE LOGIN add mail varchar(30);

SELECT * FROM LOGIN
WHERE id='a';

