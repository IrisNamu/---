SELECT
	name,
	stu_id
FROM
	STUDENT
WHERE
	"YEAR" = 3
	OR "YEAR" = 4
ORDER BY
	STU_ID;
	
SELECT *
FROM STUDENT s 
WHERE "YEAR" =3 OR "YEAR" =5
ORDER BY STU_ID;

SELECT
	name,
	stu_id
FROM
	STUDENT s
WHERE
	"YEAR" = 3
	OR "YEAR" = 4
ORDER BY
	NAME DESC, --DESC 占쎈쐻占쎈윥占쎈㎍占쎈쐻占쎈윥占쎌맽占쎈쐻占쎈짗占쎌굲�뜝�럥�맶�뜝�럥�쑅�뜝�럥�젛�뜝�럥�맶�뜝�럥�쑅�뜝�럥鍮℡뜝�럥夷у뜝�럥利닷뜝�럥猷쒙옙�쐻占쎈윪�뤃�먯삕占쎌맶�뜝�럥�쑅嶺뚮쵓�삕
	STU_ID;
	


